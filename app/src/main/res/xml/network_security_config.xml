<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!--Android P 限制了明文流量的网络请求，非加密的流量请求都会被系统禁止掉 或者http换成https -->
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates src="system" />
            <certificates src="user" />
        </trust-anchors>
    </base-config>
</network-security-config>
